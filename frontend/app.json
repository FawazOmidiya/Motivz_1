{
  "expo": {
    "name": "Motivz",
    "slug": "frontend",
    "version": "1.6.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "motivz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fomidiya.frontend",
      "buildNumber": "5",
      "icon": "./assets/images/appstore.png",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app requires access to your location to show nearby clubs.",
        "NSLocationAlwaysUsageDescription": "We use your location to show you nearby places even when the app is in the background.",
        "UIBackgroundModes": ["remote-notification"]
      },
      "entitlements": {
        "aps-environment": "production"
      },
      "associatedDomains": ["applinks:42P3N5W766.motivz-frontend.expo.app"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIcons (1)/playstore.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fomidiya.frontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appstore.png"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/AppIcons (1)/Assets.xcassets/AppIcon.appiconset/1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-build-properties",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share them with your friends.",
          "microphonePermission": "The app accesses your microphone to let you share them with your friends."
        }
      ],
      "expo-web-browser",
      "expo-video",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.973030992974-a00q54c5i2vor0i2ggdtovtcaklgnbb2",
          "iosClientId": "973030992974-a00q54c5i2vor0i2ggdtovtcaklgnbb2.apps.googleusercontent.com"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "66836f2a-6d2a-490d-ad14-7624483fff56"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/66836f2a-6d2a-490d-ad14-7624483fff56"
    }
  }
}
