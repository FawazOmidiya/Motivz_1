{
  "expo": {
    "name": "Motivz",
    "slug": "frontend",
    "version": "1.6.3",
    "orientation": "portrait",
    "icon": "./assets/AppIcons/Assets.xcassets/AppIcon.appiconset/1024.png",
    "scheme": "motivz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fomidiya.frontend",
      "buildNumber": "6",
      "icon": "./assets/AppIcons/Assets.xcassets/AppIcon.appiconset/1024.png",
      "splash": {
        "image": "./assets/images/splash-screen.png",
        "resizeMode": "cover",
        "backgroundColor": "#000000"
      },
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app requires access to your location to show nearby clubs.",
        "NSLocationAlwaysUsageDescription": "We use your location to show you nearby places even when the app is in the background.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "entitlements": {
        "aps-environment": "production"
      },
      "associatedDomains": [
        "applinks:fomidiya-frontend.expo.app",
        "applinks:themotivz.com"
      ]
    },
    "android": {
      "icon": "./assets/AppIcons/playstore.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIcons/playstore.png"
      },
      "package": "com.fomidiya.frontend",
      "versionCode": 5
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/appstore.png"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000",
          "enableFullScreenImage_legacy": true,
          "ios": {
            "enableFullScreenImage_legacy": true
          }
        }
      ],
      "expo-secure-store",
      "expo-build-properties",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share them with your friends.",
          "microphonePermission": "The app accesses your microphone to let you share them with your friends."
        }
      ],
      "expo-web-browser",
      "expo-video",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.973030992974-a00q54c5i2vor0i2ggdtovtcaklgnbb2",
          "iosClientId": "973030992974-a00q54c5i2vor0i2ggdtovtcaklgnbb2.apps.googleusercontent.com"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "66836f2a-6d2a-490d-ad14-7624483fff56"
      }
    },
    "expo-router": {
      "root": "./app",
      "origin": false
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/66836f2a-6d2a-490d-ad14-7624483fff56"
    }
  }
}
